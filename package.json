{
  "name": "react-node-express",
  "version": "0.0.9",
  "description": "play react node express",
  "main": "index.js",
  "scripts": {
    "build": "yarn clean && cross-env NODE_ENV=production webpack --config webpack.prod.js",
    "build:dev": "yarn clean && cross-env NODE_ENV=development webpack --config webpack.dev.js",
    "build:dev:client": "cross-env NODE_ENV=development webpack --config webpack.dev.client.js",
    "build:dev:server": "cross-env NODE_ENV=development webpack --config webpack.dev.server.js",
    "clean": "node cleandist.js",
    "clean:modules": "node cleanmodules.js",
    "lint": "eslint src",
    "start": "node dist/server.js",
    "start:dev": "nodemon dist/server.js",
    "start:prod": "yarn build && yarn start",
    "bounce": "yarn clean:modules && yarn",
    "publish:heroku": "yarn build && git push heroku master"
  },
  "author": "M Lo <hello@websitesbymario.com>",
  "license": "MIT",
  "dependencies": {
    "amazon-cognito-identity-js": "^3.0.7",
    "aws-sdk": "^2.392.0",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-fileupload": "^1.1.1-alpha.1",
    "mongoose": "^5.4.4",
    "morgan": "^1.9.1",
    "node-fetch": "^2.3.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-dropzone": "^8.0.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.4",
    "babel-polyfill": "^6.26.0",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "fs-extra": "^7.0.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.4",
    "nodemon": "^1.18.9",
    "webpack": "^4.23.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
